<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
    </startup>
  <appSettings>
    <add key="ScriptPath" value="Script\Stop.ps1"/>
    <add key="ScriptArgs" value=""/>
    <add key="ExePath" value="PowerShell"/>
    <add key="RequireTransactionID" value="false"/>
    <add key="RequireAppRootPath" value="true"/>
    <add key="ShouldUseShortPath" value="false"/>
    <add key="ShouldExitOnComplete" value="true"/>
    <add key="ShouldShellExecute" value="false"/>
    <add key="ShouldCreateNewWindow" value="true"/>
    <add key="ArgumentTemplate" value="-ExecutionPolicy ByPass -NoExit -File &quot;{0}&quot;"/>
    <add key="LogFilePathTemplate" value="\Log\{0}.log"/>
  </appSettings>
</configuration>