<Page x:Uid="Page_1" x:Class="DIS.Presentation.KMT.ReportKeysView.ReportKeysSelection"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:res="clr-namespace:DIS.Presentation.KMT.Properties"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:Behaviors="clr-namespace:DIS.Presentation.KMT.Behaviors"
      xmlns:my="clr-namespace:DIS.Presentation.KMT.Controls"
      mc:Ignorable="d" 
      Width="Auto" 
      Height="Auto"
      >

    <Grid x:Uid="grdReportSearch">
        <Grid.RowDefinitions>
            <RowDefinition  Height="auto"/>
            <RowDefinition  Height="auto"/>
            <RowDefinition  Height="auto"/>
        </Grid.RowDefinitions>
        <my:SearchControl x:Name="searchControl" Grid.Row="0" x:Uid="searchControl"/>
        <my:KeysTabControl x:Name="keysTab" HorizontalAlignment="Stretch" Grid.Row="1" Margin="0,20,0,0" x:Uid="keysTab"/>
        <!--<StackPanel x:Uid="stpReportTo" Grid.Row="2" Margin="0,8,0,0" Orientation="Horizontal">
            <CheckBox x:Uid="checkBoxIsCompatibleModeEnabled" x:Name="checkBoxIsCompatibleModeEnabled" VerticalAlignment="Center" Content="Do NOT report serial numbers" IsChecked="{Binding IsReportingSerialNumbers}" />
        </StackPanel>-->
    </Grid>
</Page>
