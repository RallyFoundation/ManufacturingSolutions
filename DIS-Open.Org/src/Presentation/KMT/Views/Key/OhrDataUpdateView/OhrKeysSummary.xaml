<Page
    xmlns:my="clr-namespace:DIS.Presentation.KMT.Controls" 
    x:Uid="Page" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
    x:Class="DIS.Presentation.KMT.OhrDataUpdateView.OhrKeysSummary"
    xmlns:res="clr-namespace:DIS.Presentation.KMT.Properties"
    x:Name="Page"
    WindowTitle="Page"
    FlowDirection="LeftToRight"
    Width="Auto" Height="Auto">

    <Grid  x:Uid="grdRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <my:OhrKeyOperationResultsControl x:Uid="searchResults" x:Name="searchResults" Grid.Row="1"/>
        <Label x:Uid="lblimportResult" Margin="0,0,0,0" Content="{Binding ResultMsg}" Grid.Row="2" Style="{StaticResource LblNormalSty}"/>
    </Grid>
</Page>
