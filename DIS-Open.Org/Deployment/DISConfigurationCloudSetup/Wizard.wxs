<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <!--<?Include $(var.ProjectDir)\Properties.wxs ?>-->
	<Fragment>
    <UI Id="InstallationWizard">
      <TextStyle Id="WixUI_Font_Normal" FaceName="Tahoma" Size="8" />
      <TextStyle Id="WixUI_Font_Bigger" FaceName="Tahoma" Size="12" />
      <TextStyle Id="WixUI_Font_Title" FaceName="Tahoma" Size="9" Bold="yes" />
      <Property Id="DefaultUIFont" Value="WixUI_Font_Normal" />
      <Property Id="WixUI_Mode" Value="Mondo" />
      
      <DialogRef Id="MsiRMFilesInUse" />
      <DialogRef Id="FatalError" />
      <DialogRef Id="FilesInUse" />
      <DialogRef Id="InstallDirDlg" />
      <DialogRef Id="ErrorDlg" />
      <DialogRef Id="FatalError" />
      <DialogRef Id="FilesInUse" />
      <DialogRef Id="MsiRMFilesInUse" />
      <DialogRef Id="PrepareDlg" />
      <DialogRef Id="ProgressDlg" />
      <DialogRef Id="ResumeDlg" />
      <DialogRef Id="UserExit" />
      <DialogRef Id="BrowseDlg" />

      <DialogRef Id="InvalidFeatureSelectionDlg" />
      <DialogRef Id="RequireIisDlg" />
      <DialogRef Id="RequireIisDlg" />

      <DialogRef Id="DatabaseComponentDialog" />
      <DialogRef Id="DISConfigurationCloudDialog"/>

      <Publish Dialog="BrowseDlg" Control="OK" Event="DoAction" Value="WixUIValidatePath" Order="3">1</Publish>
      <Publish Dialog="BrowseDlg" Control="OK" Event="SpawnDialog" Value="InvalidDirDlg" Order="4">
        <![CDATA[
        WIXUI_INSTALLDIR_VALID<>"1"
        ]]>
      </Publish>

      <Publish Dialog="ExitDialog" Control="Finish" Event="EndDialog" Value="Return" Order="999">1</Publish>

      <Publish Dialog="WelcomeDlg" Control="Next" Event="NewDialog" Value="LicenseAgreementDlg">
        <![CDATA[
        NOT Installed 
        AND PREVIOUSVERSIONSINSTALLED = ""
        ]]>
      </Publish>

      <Publish Dialog="LicenseAgreementDlg" Control="Back" Event="NewDialog" Value="WelcomeDlg">1</Publish>
      <Publish Dialog="LicenseAgreementDlg" Control="Next" Event="NewDialog" Value="InstallDirDlg">
        <![CDATA[
        LicenseAccepted = "1"
        ]]>
      </Publish>

      <Publish Dialog="InstallDirDlg" Control="Back" Event="NewDialog" Value="LicenseAgreementDlg">1</Publish>
      <Publish Dialog="InstallDirDlg" Control="Next" Event="SetTargetPath" Value="[WIXUI_INSTALLDIR]" Order="1">1</Publish>
      <Publish Dialog="InstallDirDlg" Control="Next" Event="DoAction" Value="WixUIValidatePath" Order="2">
        <![CDATA[
         NOT WIXUI_DONTVALIDATEPATH
        ]]>
      </Publish>
      <Publish Dialog="InstallDirDlg" Control="Next" Event="SpawnDialog" Value="InvalidDirDlg" Order="3">
        <![CDATA[
        NOT WIXUI_DONTVALIDATEPATH 
        AND WIXUI_INSTALLDIR_VALID<>"1"
        ]]>
      </Publish>
      <Publish Dialog="InstallDirDlg" Control="Next" Event="NewDialog" Value="CustomizeDlg" Order="4">
        <![CDATA[
        WIXUI_DONTVALIDATEPATH
        OR WIXUI_INSTALLDIR_VALID="1" 
        ]]>
      </Publish>
      <Publish Dialog="InstallDirDlg" Control="ChangeFolder" Property="_BrowseProperty" Value="[WIXUI_INSTALLDIR]" Order="1">1</Publish>
      <Publish Dialog="InstallDirDlg" Control="ChangeFolder" Event="SpawnDialog" Value="BrowseDlg" Order="2">1</Publish>

      <Publish Dialog="CustomizeDlg" Control="Back" Event="NewDialog" Value="MaintenanceTypeDlg">Installed</Publish>
      <Publish Dialog="CustomizeDlg" Control="Back" Event="NewDialog" Value="InstallDirDlg">NOT Installed</Publish>
      <Publish Dialog="CustomizeDlg" Control="Next" Event="SpawnDialog" Value="InvalidFeatureSelectionDlg" Order="1">
        <![CDATA[
        NOT (&DatabaseFeature=3) 
        AND NOT (&ApplicationFeature=3) 
        AND NOT Installed
        ]]></Publish>
      <Publish Dialog="CustomizeDlg" Control="Next" Event="SpawnDialog" Value="RequireIisDlg" Order="2">
        <![CDATA[
        NOT Installed 
        AND (&ApplicationFeature=3) 
        AND (NOT IISMAJORVERSION OR (IISMAJORVERSION <> "#6" AND IISMAJORVERSION <> "#7" AND IISMAJORVERSION <> "#8"))
        ]]>
      </Publish>
      <Publish Dialog="CustomizeDlg" Control="Next" Event="NewDialog" Value="DatabaseComponentDialog" Order="3">
        <![CDATA[
        &DatabaseFeature=3
        AND NOT Installed
        ]]>
      </Publish>
      <Publish Dialog="CustomizeDlg" Control="Next" Event="NewDialog" Value="DISConfigurationCloudDialog" Order="4">
        <![CDATA[
        &ApplicationFeature=3
        AND NOT &DatabaseFeature=3
        AND NOT Installed
        ]]>
      </Publish>

      <Publish Dialog="DatabaseComponentDialog" Control="Next" Event="NewDialog" Value="DISConfigurationCloudDialog" Order="1">
        <![CDATA[
        &ApplicationFeature=3
        ]]>
      </Publish>
      <Publish Dialog="DatabaseComponentDialog" Control="Next" Event="NewDialog" Value="VerifyReadyDlg" Order="2">
        <![CDATA[
        NOT &ApplicationFeature=3
        ]]>
      </Publish>
      <Publish Dialog="DatabaseComponentDialog" Control="Back" Event="NewDialog" Value="CustomizeDlg">1</Publish>
      
      <Publish Dialog="DISConfigurationCloudDialog" Control="Next" Event="NewDialog" Value="VerifyReadyDlg">1</Publish>
      <Publish Dialog="DISConfigurationCloudDialog" Control="Back" Event="NewDialog" Value="DatabaseComponentDialog" Order="1">
        <![CDATA[
        &DatabaseFeature=3
        ]]>
      </Publish>
      <Publish Dialog="DISConfigurationCloudDialog" Control="Back" Event="NewDialog" Value="CustomizeDlg" Order="2">
        <![CDATA[
        NOT &DatabaseFeature=3
        ]]>
      </Publish>
      
      <Publish Dialog="VerifyReadyDlg" Control="Back" Event="NewDialog" Value="DISConfigurationCloudDialog" Order="1">1</Publish>

      <Publish Dialog="MaintenanceWelcomeDlg" Control="Next" Event="NewDialog" Value="MaintenanceTypeDlg">1</Publish>

      <Publish Dialog="MaintenanceTypeDlg" Control="ChangeButton" Event="NewDialog" Value="CustomizeDlg">1</Publish>
      <Publish Dialog="MaintenanceTypeDlg" Control="RepairButton" Event="NewDialog" Value="VerifyReadyDlg">1</Publish>
      <Publish Dialog="MaintenanceTypeDlg" Control="RemoveButton" Event="NewDialog" Value="VerifyReadyDlg">1</Publish>
      <Publish Dialog="MaintenanceTypeDlg" Control="Back" Event="NewDialog" Value="MaintenanceWelcomeDlg">1</Publish>

      <Property Id="ARPNOMODIFY" Value="1" />
    </UI>

    <UIRef Id="WixUI_Common" />
  </Fragment>
</Wix>