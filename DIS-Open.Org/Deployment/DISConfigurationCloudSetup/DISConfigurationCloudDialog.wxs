<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
	<Fragment>
    <UI>
      <Dialog Id="DISConfigurationCloudDialog" Width="370" Height="270" Title="!(loc.InstallDirDlg_Title)">
        <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="!(loc.WixUINext)">
          <Condition Action="disable"><![CDATA[APP_SERVER_SITE_PORT_NUMBER = "" OR BUSINESS_DB_BACKUP_LOCATION = "" OR APP_SERVER_SITE_PORT_NUMBER < 1 OR APP_SERVER_SITE_PORT_NUMBER > 65535]]></Condition>
          <Condition Action="enable"><![CDATA[APP_SERVER_SITE_PORT_NUMBER > 0 AND APP_SERVER_SITE_PORT_NUMBER < 65536 AND BUSINESS_DB_BACKUP_LOCATION <> ""]]></Condition>
        </Control>
        <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="!(loc.WixUIBack)" />
        <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="!(loc.WixUICancel)">
          <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
        </Control>
        <Control Id="Description" Type="Text" X="25" Y="23" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="!(loc.CloudConfigurationDescription)" />
        <Control Id="Title" Type="Text" X="15" Y="6" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="{\WixUI_Font_Title}!(loc.CloudConfigurationTitle)" />
        <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="!(loc.InstallDirDlgBannerBitmap)" />
        <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
        <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
        <Control Type="GroupBox" Id="Port" Text="!(loc.Port)" Width="330" Height="90" X="20" Y="60" />
        <Control Id="AppServerPortNumberLabel" Type="Text" X="40" Y="80" Width="120" Height="17" NoPrefix="yes" Text="!(loc.CloudHttpPortNumberInput)"> </Control>
        <Control Type="Edit" Id="AppServerPortNumber" Property="APP_SERVER_SITE_PORT_NUMBER" Width="50" Height="15" X="170" Y="77"  Integer="yes"></Control>
        <Control Id="DBBackupLocationLabel" Type="Text" X="40" Y="120" Width="120" Height="17" NoPrefix="yes" Text="!(loc.CloudDatabaseBackupLocationInput)"></Control>
        <Control Type="Edit" Id="DBBackupLocation" Property="BUSINESS_DB_BACKUP_LOCATION" Width="150" Height="15" X="170" Y="117"></Control>
      </Dialog>
    </UI>
	</Fragment>
</Wix>