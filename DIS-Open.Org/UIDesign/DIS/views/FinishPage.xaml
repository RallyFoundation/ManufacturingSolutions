<Page
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="DIS.FinishPage"
	x:Name="Page"
	WindowTitle="Page"
	FlowDirection="LeftToRight"
	Width="auto" Height="auto">

	<Grid x:Name="LayoutRoot">
		<Grid.RowDefinitions>
			<RowDefinition Height="20"/>
			<RowDefinition Height="70"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<Label Content="The selected keys will be exported to following location :" Grid.Row="1" Margin="0,0,8,0" Padding="30,5,5,5" VerticalContentAlignment="Bottom"/>
		<Label Content="D:\Data" Grid.Row="2" VerticalAlignment="Top" Height="74.96" d:LayoutOverrides="Width" Padding="30,5,5,5" FontWeight="Bold" FontStyle="Italic"/>
	</Grid>
</Page>